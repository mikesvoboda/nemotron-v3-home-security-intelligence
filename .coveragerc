[run]
source = backend
omit =
    backend/tests/*
    backend/examples/*
    backend/main.py
    */__pycache__/*
    */.venv/*
    */venv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
fail_under = 93
show_missing = true
precision = 2

[html]
directory = coverage/backend
