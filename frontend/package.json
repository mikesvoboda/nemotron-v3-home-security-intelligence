{
  "name": "home-security-dashboard",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:e2e": "vite --config vite.config.e2e.ts",
    "build": "tsc && vite build",
    "lint": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,json}\"",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "test": "NODE_OPTIONS='--max-old-space-size=8192' vitest",
    "test:ui": "NODE_OPTIONS='--max-old-space-size=8192' vitest --ui",
    "test:coverage": "NODE_OPTIONS='--max-old-space-size=8192' vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "generate-types": "cd .. && ./scripts/generate-types.sh",
    "generate-types:check": "cd .. && ./scripts/generate-types.sh --check",
    "validate": "npm run typecheck && npm run lint && npm run test:coverage -- --run",
    "dead-code": "knip",
    "test:mutation": "stryker run",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch",
    "analyze": "vite build --mode analyze"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@hookform/resolvers": "^5.2.2",
    "@sentry/react": "^10.34.0",
    "@tanstack/react-query": "^5.90.19",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@tremor/react": "^3.17.4",
    "clsx": "^2.1.0",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.1",
    "framer-motion": "^12.27.0",
    "lru-cache": "^11.2.4",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.71.1",
    "react-joyride": "^2.9.3",
    "react-router-dom": "^7.11.0",
    "sonner": "^2.0.7",
    "vite-plugin-pwa": "^1.2.0",
    "web-vitals": "^5.1.0",
    "workbox-window": "^7.4.0",
    "zod": "^4.3.5",
    "zustand": "^5.0.10"
  },
  "overrides": {
    "tmp": ">=0.2.4"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@stryker-mutator/api": "^8.7.1",
    "@stryker-mutator/core": "^8.7.1",
    "@stryker-mutator/typescript-checker": "^8.7.1",
    "@stryker-mutator/vitest-runner": "^9.4.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^4.0.17",
    "autoprefixer": "^10.4.17",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-security": "^3.0.1",
    "fake-indexeddb": "^6.2.5",
    "globals": "^16.5.0",
    "jsdom": "^27.3.0",
    "knip": "^5.82.1",
    "msw": "^2.12.7",
    "openapi-typescript": "^7.10.1",
    "postcss": "^8.4.33",
    "prettier": "^3.8.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "rollup-plugin-visualizer": "^6.0.5",
    "tailwindcss": "^3.4.1",
    "timezone-mock": "^1.3.6",
    "typedoc": "0.28.16",
    "typedoc-plugin-markdown": "4.9.0",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.53.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.17",
    "vitest-axe": "^0.1.0"
  }
}
